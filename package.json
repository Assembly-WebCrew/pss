{
    "name": "pss",
    "version": "0.1.0",
    "description": "Party Schedule Service",
    "repository": "https://github.com/Assembly-WebCrew/pss",
    "main": "api/index.js",
    "scripts": {
        "build": "docker-compose build",
        "test": "mocha",
        "coverage": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --colors --reporter dot test/*.js",
        "db": "NODE_ENV=development bin/setup-db.sh",
        "resetdb": "NODE_ENV=development babel-node bin/initdb.es6",
        "migrate": "knex migrate:latest",
        "start": "npm run migrate && node api"
    },
    "author": "Assembly Webcrew",
    "license": "MIT",
    "dependencies": {
        "bcrypt": "^3.0.0",
        "eslint": "^5.3.0",
        "mysql": "^2.16.0",
        "knex": "^0.15.2",
        "objection": "^1.2.2"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "winston": "^3.0.0",
        "chai": "^4.0.1",
        "chai-http": "^3.0.0",
        "istanbul": "^0.4.5",
        "mocha": "^3.4.2",
        "nodemon": "^1.18.3"
    }
}